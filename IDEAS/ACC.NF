: ACC PUSH 0
BEGIN KEY 255 AND
  CASE
    bs OF DUP IF 1- bs DUP EMIT bl EMIT EMIT THEN ENDOF
    cr OF RDROP DUP PUSH ENDOF
      DUP bl U< 0=
      IF DUP EMIT PUSH 2DUP + POP SWAP C! 1+ DUP THEN
  ENDCASE DUP R@ =
UNTIL RDROP ;


: ACC PUSH xchg I+ PUSH J BEGIN KEY 255 AND  CASE
   bs OF DUP J - IF 1- bs DUP EMIT bl EMIT EMIT THEN ENDOF
   cr OF RDROP DUP I+ PUSH ENDOF      DUP bl U< 0= 
   IF DUP EMIT SWAP C!+ DUP THEN
   ENDCASE DUP I = 
 UNTIL DROP POP I- XCHG POP ;

: I- J - ; 
: ACC OVER PUSH + BEGIN KEY W>B CASE bs OF OVER I- IF 1- bs DUP
EMIT SPACE EMIT THEN ENDOF cr OF XCHG DROP I ENDOF DUP bl U< 0=
IF DUP EMIT C!T+ DUP THEN ENDCASE DUP I = UNTIL 
: EXIT POP : DROPX DROP ;
